<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.xyurp.system.bean">
	<class name="cn.xyurp.system.bean.SysACL" table="t_sysacl">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="principalType" length="20"/>
		<property name="principalId" />
		<property name="sysModuleId" />
		<property name="aclState" />
		<property name="aclTriState" />
	</class>
</hibernate-mapping>