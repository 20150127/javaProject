<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.xyurp.zs.bean">
    <class name="cn.xyurp.zs.bean.Link" table="t_zs_link">
        <id name="id">
            <generator class="native" />
        </id>
        <property name="pxh" type="java.lang.Integer"/>
        <property name="title" type="java.lang.String" length="100"/>
        <property name="path" type="java.lang.String" length="500"/>
        <property name="remark" type="java.lang.String" length="3000"/>
    </class>
</hibernate-mapping>
