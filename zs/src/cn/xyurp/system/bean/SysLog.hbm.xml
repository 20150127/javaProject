<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.xyurp.system.bean">
    <class name="cn.xyurp.system.bean.SysLog" table="t_syslog">
        <id name="id" type="java.lang.Long">
            <generator class="native" />
        </id>
        <property name="sysUser" type="java.lang.String" length="30"/>
        <property name="ip" type="java.lang.String" length="18" />
        <property name="sysTime" length="50" type="java.lang.String" />
        <property name="sysModule" length="50" type="java.lang.String"/>
        <property name="sysMethod" length="50" type="java.lang.String"/>      
    </class>
</hibernate-mapping>